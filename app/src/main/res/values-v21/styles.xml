<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="MyCustomTextView" parent="android:Widget.TextView">
        <item name="android:textColor">@color/appThemeYellow</item>
        <item name="android:background">@color/appThemeBgColorLight</item>
        <item name="android:elevation">10dp</item>
        <item name="android:gravity">center</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">18sp</item>
    </style>


    <style name="MySeekBar" parent="Widget.AppCompat.SeekBar">
        <item name="android:indeterminateOnly">false</item>
        <item name="android:progressDrawable">@drawable/custom_seek_bar</item>
        <item name="android:minHeight">10dip</item>
        <item name="android:progressTint">@color/colorConfirmedStatus</item>
        <item name="android:maxHeight">10dip</item>
        <item name="android:thumb">@drawable/circular</item>
        <item name="android:focusable">true</item>
    </style>

